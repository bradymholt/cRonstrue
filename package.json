  },
  "prettier": {
    "printWidth": 120
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "handlebars": "^4.7.6",
    "marked": "^4.0.10",
    "mocha": "^8.1.3",
    "pixl-xml": "^1.0.13",
    "prettier": "^2.1.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
